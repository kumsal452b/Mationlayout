<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/icon_1"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:scaleType="center"
        android:src="@drawable/cameraretro"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icon_2"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:scaleType="center"
        android:src="@drawable/gopuram"
        app:layout_constraintEnd_toStartOf="@+id/icon_3"
        app:layout_constraintStart_toEndOf="@+id/icon_1"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icon_3"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:scaleType="center"
        android:src="@drawable/instagram"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/icon_4"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="45dp"
        android:scaleType="center"
        android:src="@drawable/phoenixframework"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_1" />

    <ImageView
        android:id="@+id/icon_5"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:scaleType="center"
        android:src="@drawable/projectdiagram"
        app:layout_constraintEnd_toStartOf="@+id/icon_6"
        app:layout_constraintStart_toEndOf="@+id/icon_4"
        app:layout_constraintTop_toTopOf="@+id/icon_4" />

    <ImageView
        android:id="@+id/icon_6"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:scaleType="center"
        android:src="@drawable/telegram"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/icon_5" />

    <ImageView
        android:id="@+id/icon_7"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_marginTop="45dp"
        android:scaleType="center"
        android:src="@drawable/telegramplane"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_4" />

    <ImageView
        android:id="@+id/icon_8"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:scaleType="center"
        android:src="@drawable/tram"
        app:layout_constraintEnd_toStartOf="@+id/icon_9"
        app:layout_constraintStart_toEndOf="@+id/icon_7"
        app:layout_constraintTop_toTopOf="@+id/icon_7" />

    <ImageView
        android:id="@+id/icon_9"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:scaleType="center"
        android:src="@drawable/wifi"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/icon_8" />

    <ImageView
        android:id="@+id/icon_0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="168dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/icon_8"
        app:srcCompat="@drawable/expand" />


</androidx.constraintlayout.widget.ConstraintLayout>